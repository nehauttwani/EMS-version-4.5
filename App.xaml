             xmlns:local="clr-namespace:EMS"
             StartupUri="Views/MainWindow.xaml"
             xmlns:views="clr-namespace:EMS.Views"
             xmlns:viewmodels="clr-namespace:EMS.ViewModel">
    <Application.Resources>
        <ResourceDictionary>
